package com.apirest_microservicio.model;

public class Curso {
	private String nombre;
	private int duracion;
	private String horario;
	public Curso(String nombre, int duracion, String horario) {
		super();
		this.nombre = nombre;
		this.duracion = duracion;
		this.horario = horario;
	}
	
	public Curso() {
		
	}
	
	
	
}
