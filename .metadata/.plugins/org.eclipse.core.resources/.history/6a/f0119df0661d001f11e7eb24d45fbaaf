		// TODO Auto-generated method stub

	}

	@Override
	public boolean eliminarContacto(int idContacto) {
		// TODO Auto-generated method stub
		return false;
	}

	@Override
	public Contacto buscarContacto(int idContacto) {
		// TODO Auto-generated method stub
		return null;
	}

}
